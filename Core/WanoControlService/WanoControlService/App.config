  <connectionStrings>
    <add name="DBConnectionString" connectionString="Server=localhost;Port=5432;User Id=postgres;Password=Djm600kt;Database=Main;CommandTimeout=3600;Timeout=15;MinPoolSize=5;MaxPoolSize=60;SearchPath=wano_main;ApplicationName=WanoControlService" providerName="Npgsql" />
  </connectionStrings>
  <runtime>
